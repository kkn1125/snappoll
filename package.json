{
  "name": "snappoll",
  "version": "0.0.63",
  "description": "SnapPoll(스냅폴)은 사용자들이 설문, 투표를 쉽게 만들고 참여할 수 있는 플랫폼입니다.",
  "author": {
    "name": "devkimson",
    "email": "chaplet01@gmail.com",
    "url": "https://kkn1125.github.io/",
    "profile": "https://avatars.githubusercontent.com/u/71887242?v=4"
  },
  "keywords": [
    "easy-survey",
    "share-survey",
    "survey",
    "vote",
    "free",
    "result-graph",
    "snappoll"
  ],
  "private": true,
  "license": "MIT",
  "scripts": {
    "seed": "npx prisma db seed",
    "dev": "concurrently --kill-others-on-fail \"npm run start:dev\" \"cd views/ && npm run dev\"",
    "prebuild": "npx prisma generate",
    "build": "nest build",
    "onlybuild": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:prisma": "prisma-case-format -c .prisma-case-format -f prisma/schema.prisma -p",
    "start": "nest start",
    "start:hmr": "cross-env NODE_ENV=development nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "deploy": "ctype apply -f temp/realtime-poll.yaml"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^11.1.2",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.2",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.2",
    "@nestjs/platform-socket.io": "^11.1.2",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^11.1.2",
    "@prisma/client": "^6.8.2",
    "axios": "^1.9.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "express-handlebars": "^8.0.3",
    "handlebars": "^4.7.8",
    "hbs": "^4.2.0",
    "jsonwebtoken": "^9.0.2",
    "ms": "^2.1.3",
    "nodemailer": "^7.0.3",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "react-helmet-async": "^2.0.5",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.2",
    "@prisma/generator-helper": "^6.8.2",
    "@types/compression": "^1.8.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^5.0.2",
    "@types/hbs": "^4.0.4",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/ms": "^2.1.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.15.27",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "prisma": "^6.8.2",
    "prisma-case-format": "^2.2.1",
    "prisma-class-generator": "^0.2.11",
    "run-script-webpack-plugin": "^0.2.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "webpack": "^5.99.9",
    "webpack-node-externals": "^3.0.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
